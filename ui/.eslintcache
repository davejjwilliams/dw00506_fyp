      <ul className='right hide-on-med-and-down'>\r\n            {isAuthenticated ? authLinks : guestLinks}\r\n          </ul>\r\n        </div>\r\n      </nav>\r\n\r\n      <ul className='sidenav' id='mobile-demo'>\r\n        <li>\r\n          <Link to='/about'>About</Link>\r\n        </li>\r\n        {isAuthenticated ? authLinks : guestLinks}\r\n      </ul>\r\n    </Fragment>\r\n  );\r\n};\r\n\r\nNavbar.propTypes = {\r\n  title: PropTypes.string.isRequired,\r\n  icon: PropTypes.string\r\n};\r\n\r\nNavbar.defaultProps = {\r\n  title: 'Manutrackture',\r\n  icon: 'house'\r\n};\r\n\r\nexport default Navbar;\r\n","C:\\Users\\Dave\\ethereumworkspace\\dw00506_fyp\\ui\\src\\components\\pages\\Home.js",["116","117"],"import React, { Fragment, useEffect, useContext } from 'react';\r\nimport Products from '../products/Products';\r\nimport AuthContext from '../../context/auth/authContext';\r\n\r\nconst Home = () => {\r\n  const authContext = useContext(AuthContext);\r\n\r\n  const { loading, user, loadUser } = authContext;\r\n\r\n  useEffect(() => {\r\n    loadUser();\r\n  }, []);\r\n\r\n  return (\r\n    <Fragment>\r\n      <h1>Your Products</h1>\r\n      <div>\r\n        {user && user.role === 'customer' && (\r\n          <p>Enter a product code to follow a new product.</p>\r\n        )}\r\n        {user && user.role === 'seller' && (\r\n          <p>Add a new product from the top menu button.</p>\r\n        )}\r\n        {user && user.role === 'manufacturer' && (\r\n          <p>Products sellers have included you in will appear below.</p>\r\n        )}\r\n        <Products />\r\n      </div>\r\n    </Fragment>\r\n  );\r\n};\r\nexport default Home;\r\n","C:\\Users\\Dave\\ethereumworkspace\\dw00506_fyp\\ui\\src\\components\\pages\\About.js",[],"C:\\Users\\Dave\\ethereumworkspace\\dw00506_fyp\\ui\\src\\context\\product\\ProductState.js",[],"C:\\Users\\Dave\\ethereumworkspace\\dw00506_fyp\\ui\\src\\context\\product\\productReducer.js",[],"C:\\Users\\Dave\\ethereumworkspace\\dw